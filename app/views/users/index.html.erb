<h1>Users</h1>

<table>
  <thead>
  <tr>
    <th>email</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.email %></td>
      <td><%= link_to 'show', user_path(user) %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<%= paginate @users %>

<br>
